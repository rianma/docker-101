{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach localhost:6000",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "address": "0.0.0.0",
      "port": 16000,
      "protocol": "inspector"
    }
  ]
}